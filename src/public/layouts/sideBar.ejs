      function handleIconClick(btnId) {
        const Configuration = document.getElementById(`home-sidebar`);
        const Chanels = document.getElementById(`functions-sidebar`);
        const Guild = document.getElementById(`guildDB-sidebar`);
        const User = document.getElementById(`serverInfos-sidebar`);
        const Usage = document.getElementById(`config-sidebar`);
        const ConfigurationHash = document.getElementById(`hashtag-home`);
        const ChanelsHash = document.getElementById(`hashtag-functions`);
        const GuildHash = document.getElementById(`hashtag-guildDB`);
        const UserHash = document.getElementById(`hashtag-serverInfos`);
        const UsageHash = document.getElementById(`hashtag-config`);
        const hashtag = document.getElementById(`hashtag-${btnId}`);
        const sidebar = document.getElementById(`${btnId}-sidebar`);

        if (!Configuration.classList.contains("hidden")) {
          Configuration.classList.toggle("hidden");
          ConfigurationHash.classList.toggle("hidden");
        }

        if (!Chanels.classList.contains("hidden")) {
          Chanels.classList.toggle("hidden");
          ChanelsHash.classList.toggle("hidden");
        }
        if (!Guild.classList.contains("hidden")) {
          Guild.classList.toggle("hidden");
          GuildHash.classList.toggle("hidden");
        }
        if (!User.classList.contains("hidden")) {
          User.classList.toggle("hidden");
          UserHash.classList.toggle("hidden");
        }
        if (!Usage.classList.contains("hidden")) {
          Usage.classList.toggle("hidden");
          UsageHash.classList.toggle("hidden");
        }
        sidebar.classList.toggle("hidden");
        hashtag.classList.toggle("hidden");
      }

      function toggleDropdown(dropdownId) {
        var dropdown = document.getElementById(dropdownId + "-dropdown");
        //var chevron = document.getElementById(dropdownId + "-chevron");

        if (
          dropdown.style.display === "none" ||
          dropdown.style.display === ""
        ) {
          dropdown.style.display = "block";
          //chevron.className = "fas fa-chevron-up text-gray-400"; // adjust class for open state
        } else {
          dropdown.style.display = "none";
          //chevron.className = "fas fa-chevron-down text-gray-400"; // adjust class for closed state
        }
      }

